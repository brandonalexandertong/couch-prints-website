<section class="news-scroll">
    {%- for press in collections.press | reverse -%}
    <article class="top">
        <a href="{{ press.data.link }}" target="_blank"><h2>{{ press.data.datePublished }} | {{ press.data.publication }}<br> {{ press.data.title }} </h2>
        </a>
        
        {% if press.data.tags.includes("video") %}
            <iframe width="560" height="315" src="{{ press.data.videoLink }}" title="YouTube video player" frameborder="0" allow="accelerometer; autoplay; clipboard-write; encrypted-media; gyroscope; picture-in-picture" allowfullscreen></iframe>
        {% else %}
            <a href="{{ press.data.link }}" target="_blank">
                <img src="./assets/images/{{ press.data.image }}" alt="Tell-U">
            </a>
        {% endif %}
        {{ press.templateContent | safe }}
    </article>
    {%- endfor -%}
</section>


